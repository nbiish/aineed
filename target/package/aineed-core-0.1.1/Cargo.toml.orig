[package]
name = "aineed-core"
version = "0.1.1"
edition = "2021"
authors = ["Nbiish <nbiish@umich.edu>"]
description = "Core library for the aineed AI assistant tool"
repository = "https://github.com/nbiish/aineed"
license = "MIT"
readme = "../../README.md"
keywords = ["ai", "openai", "anthropic", "llm", "api"]
categories = ["api-bindings"]

[dependencies]
async-trait = "0.1"
base64 = "0.21"
bytes = "1.5"
chrono = "0.4"
dirs = "5.0"
futures-util = "0.3"
reqwest = { version = "0.11", features = ["json", "stream"] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
tokio = { version = "1.0", features = ["full"] } 